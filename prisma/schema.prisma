generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int    @id @default(autoincrement())
  user     String
  email    String @unique
  password String
}

model AnimesRomance{
  id Int @id @default(autoincrement())
  title String
  average String
  synopisis String @db.Text
  opinionNinja String? @db.Text
  capeImage String
}